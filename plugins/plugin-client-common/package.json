{
  "name": "@kui-shell/plugin-client-common",
  "version": "8.6.0",
  "description": "Kui plugin that offers stylesheets",
  "license": "Apache-2.0",
  "author": "Mengting Yan",
  "homepage": "https://github.com/IBM/kui#readme",
  "bugs": {
    "url": "https://github.com/IBM/kui/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IBM/kui.git"
  },
  "keywords": [
    "kui",
    "plugin"
  ],
  "main": "dist/index.js",
  "module": "mdist/index.js",
  "types": "mdist/index.d.ts",
  "dependencies": {
    "@carbon/icons-react": "^10.9.2",
    "@patternfly/react-core": "^3.153.13",
    "@patternfly/react-table": "^2.28.39",
    "carbon-components": "^10.10.2",
    "carbon-components-react": "^7.10.2",
    "carbon-icons": "^7.0.7",
    "monaco-editor": "0.18.1",
    "monaco-editor-webpack-plugin": "1.7.0",
    "react-markdown": "^4.3.1",
    "react-split-pane": "^0.1.91",
    "tmp": "0.1.0"
  },
  "kui": {
    "webpack": {
      "plugins": [
        {
          "plugin": "monaco-editor-webpack-plugin",
          "options": {
            "languages": [
              "css",
              "scss",
              "less",
              "dockerfile",
              "go",
              "html",
              "handlebars",
              "javascript",
              "typescript",
              "json",
              "markdown",
              "python",
              "powershell",
              "shell",
              "yaml"
            ],
            "features": [
              "!codelens",
              "!iPadShowKeyboard",
              "!multicursor",
              "!quickCommand",
              "!quickOutline",
              "!codeAction",
              "!coreCommands",
              "!fontZoom",
              "!toggleHighContrast",
              "!colorDetector"
            ]
          }
        }
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
